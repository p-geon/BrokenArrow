#!/bin/bash

cp ${HOME}/git/BrokenArrow/.bash_aliases ${HOME}/.bash_aliases
echo "[copied] ~/.bash_aliases"
cp ${HOME}/git/BrokenArrow/.bash_profile ${HOME}/.bash_profile
echo "[copied] ~/.bash_profile"
cp ${HOME}/git/BrokenArrow/.zprofile ${HOME}/.zprofile
echo "[copied] ~/.zprofile"
cp ${HOME}/git/BrokenArrow/.zshrc ${HOME}/.zshrc
echo "[copied] ~/.zshrc"
source ~/.bashrc